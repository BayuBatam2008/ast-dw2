---
import {
	TextCardSection,
	Container,
} from '@littlesticks/odyssey-theme-components';
import Layout from '../layouts/Page.astro';
import HeroSection from '../components/sections/heros/HeroSection.astro';

const seo = {
	title: '#general | DW2',
	description: 'Channel #general',
};
---

<Layout {seo}>
	<style>
  .youtube-embed__container {
    margin-bottom: 1rem;
    aspect-ratio: 9/16;
    border-radius: 0.5rem;
    overflow: hidden;
    z-index: 1;
  }

  .youtube-embed__container iframe {
    width: 100%;
    height: 100%;
  }

  @supports not (aspect-ratio: 9 / 16) {
    .youtube-embed__container {
      position: relative;
      padding-bottom: 177.77%;
      padding-top: 30px;
      height: 0;
      overflow: hidden;
    }
    .youtube-embed__container iframe {
      position: absolute;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
    }
  }
</style>
        <Container>
		<TextCardSection>
			<h1>#general</h1>
			<p>
				Disinilah terjadi konversasi antara intelijen SPMP. Disini para intelijen memikirkan taktik dan mencari bukti.
			</p>
		</TextCardSection>
		<section>
			<div class={`youtube-embed__container`}>
                         <iframe src={`https://dw2-html.vercel.app/SPMP_General.html`} title="#general" frameborder="0" sandbox></iframe>
                        </div>
		</section>
	</Container>
</Layout>
